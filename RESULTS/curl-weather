<script id="wxcard-throw-away">
    var obs = {}, forecast = {};
    _.templateSettings = {
        interpolate: /\[\[(.+?)\]\]/g
    };
    var $card           = jQuery("#js-nowcard"),
            $card_wrapper   = jQuery("#js-nowcard"),
            $to_be_compiled = jQuery(".loc-container, .today_nowcard-condition, .today_nowcard-video"),
            template = _.template($card.html());
    if(TWC._prefetch && TWC._prefetch.promise) {
        TWC._prefetch.promise.then(function () {
            if($card_wrapper.is('.angular-load')) {
               return;
            }
            if(TWC._prefetch.models[0].data && TWC._prefetch.models[1].data &&
                    TWC._prefetch.models[0].data.vt1observation && TWC._prefetch.models[1].data.vt1dailyForecast) {
                obs = TWC._prefetch.models[0].data.vt1observation;
                forecast = TWC._prefetch.models[1].data.vt1dailyForecast;
            }

            var url         = obs.icon && TWC.wxbackgroundFactory().getBackgroundImageData(obs.icon),
                    textClass = url && (url.indexOf("fog") > -1 || url.indexOf("snow") > -1) ? 'light' : '',
                    instanceId  = $card.closest(".today_nowcard").attr("instance"),
                    settings    = (Drupal.settings.twc.instance[instanceId]) || {},
                    thumbImg    = settings.content_thumbnail_image,
                    thumbUrl    = thumbImg && thumbImg.json && thumbImg.json.variants && thumbImg.json.variants[1] || '';
            // replace individual pieces with data
            $to_be_compiled.each(function(idx, el) {
                var $el         = jQuery(el),
                        template    = _.template($el.html()),
                        htmlContent =  '<div>' + template({
                                    obs: obs,
                                    forecast: forecast,
                                    location: window.explicit_location_obj,
                                    settings: _.extend({thumb_image : thumbUrl}, settings),
                                    ui: {url: url, textClass: textClass}
                                }) + '</div>',
                        $html       = jQuery(htmlContent);
                $el.html($html.html());
                $el.removeClass("ng-cloak");
            });
            $card.find(".today_nowcard-main").css("background-image", "url('" + url + "')");
            jQuery(".today_nowcard--loader-container").addClass("loaded");
        });
    }
    jQuery("#wxcard-throw-away").remove();
</script>
</div></div>            </section>
