      languageCode = languageCode.substring(0,2);

  var currentUrlLocale = (window.location.pathname).substring(0, 7),
      isUsUrl = currentUrlLocale === '/' || !URL_LOCALE_REGREX.test(currentUrlLocale) || currentUrlLocale.charAt(3) !== '-';

  var isSpanishUsLocale = (languageCode + '-' + countryCode) === SPANISH_US_CODE,
      isCountryCodeValid = countryCode && countryCode.length === 2 && (!countryCode.match(/US/) || isSpanishUsLocale);

  if (isCountryCodeValid && isUsUrl && !window.location.search.match(/dest=US/)) {
    var currentLocale = languageCode + '-' + countryCode;
    if (languageCode && languageCode.indexOf('-') !== -1) {
        languageCode = languageCode.split('-')[0];
    }

    //== Get destination site code ==//
    var destinationSiteCode = DEFAULT_SITE_CODE;
    $.each(locales, function(key, locale) {

      if (!locale.language || (locale.language && locale.language.indexOf('-') === -1)) {
        removeController()
        return;
      }

      if (locale.language.split('-')[1].indexOf(countryCode) !== -1) {
        var matchedLocale = locales[languageCode + '-' + countryCode];
        destinationSiteCode = matchedLocale ? matchedLocale.language : null;
        removeController()
        return false;
      }
    });

    //== Settings ==//
    var fromString = "wx:siteprefs-redirect";
    var destinationUrls = {
