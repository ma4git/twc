	"ko-KR" : "https://weather.com/ko-KR/weather/today/l/{{dynamicLocId}}",
	"pl-PL" : "https://weather.com/pl-PL/pogoda/dzisiaj/l/{{dynamicLocId}}",
	"it-IT" : "https://weather.com/it-IT/tempo/oggi/l/{{dynamicLocId}}",
	"ms-MY" : "https://weather.com/ms-MY/weather/today/l/{{dynamicLocId}}",
	"hu-HU" : "https://weather.com/hu-HU/idojaras/ma/l/{{dynamicLocId}}",
	"el-GR" : "https://weather.com/el-GR/weather/today/l/{{dynamicLocId}}",
	"hi-IN" : "https://weather.com/hi-IN/weather/today/l/{{dynamicLocId}}",
	"pt-BR" : "https://weather.com/pt-BR/clima/hoje/l/{{dynamicLocId}}",
	"id-ID" : "https://weather.com/id-ID/cuaca/sekarang/l/{{dynamicLocId}}",
	"ru-RU" : "https://weather.com/ru-RU/weather/today/l/{{dynamicLocId}}",
	"zh-TW" : "https://weather.com/zh-TW/weather/today/l/{{dynamicLocId}}",
	"zh-CN" : "https://weather.com/zh-CN/weather/today/l/{{dynamicLocId}}",
	"th-TH" : "https://weather.com/th-TH/weather/today/l/{{dynamicLocId}}",
	"tr-TR" : "https://weather.com/tr-TR/kisisel/bugun/l/{{dynamicLocId}}",
	"uk-UA" : "https://weather.com/uk-UA/weather/today/l/{{dynamicLocId}}",
	"vi-VN" : "https://weather.com/vi-VN/weather/today/l/{{dynamicLocId}}",
	"sv-SE" : "https://weather.com/sv-SE/weather/today/l/{{dynamicLocId}}",
	"es-US" : "https://weather.com/es-US/tiempo/hoy/l/{{dynamicLocId}}",
	"ro-RO" : "https://weather.com/ro-RO/vreme/astazi/l/{{dynamicLocId}}",
	"fr-FR" : "https://weather.com/fr-FR/temps/aujour/l/{{dynamicLocId}}",
	"sk-SK" : "https://weather.com/sk-SK/pocasie/dnes/l/{{dynamicLocId}}",
	"es-ES" : "https://weather.com/es-ES/tiempo/hoy/l/{{dynamicLocId}}",
	"pt-PT" : "https://weather.com/pt-PT/clima/hoje/l/{{dynamicLocId}}",
	"no-NO" : "https://weather.com/no-NO/weather/today/l/{{dynamicLocId}}",
	"fi-FI" : "https://weather.com/fi-FI/weather/today/l/{{dynamicLocId}}",
	"nl-NL" : "https://weather.com/nl-NL/weer/vandaag/l/{{dynamicLocId}}",
	"da-DK" : "https://weather.com/da-DK/vejret/idag/l/{{dynamicLocId}}",
	"en-IN" : "https://weather.com/en-IN/weather/today/l/{{dynamicLocId}}",
	"cs-CZ" : "https://weather.com/cs-CZ/pocasi/dnes/l/{{dynamicLocId}}",
	"hr-HR" : "https://weather.com/hr-HR/vrijeme/danas/l/{{dynamicLocId}}",
	"ca-ES" : "https://weather.com/ca-ES/temps/avui/l/{{dynamicLocId}}"
};
    var otherRules = {
  "es": {
    "cntryCd": "AR,BO,CL,CO,CR,DO,EC,SV,GQ,GT,HN,MX,NI,PA,PY,PE,PR,US,UY,VE",
    "link": "https://weather.com/es-US/tiempo/hoy/l/{{dynamicLocId}}",
    "default": "https://weather.com/es-ES/tiempo/hoy/l/{{dynamicLocId}}"
  },

  "en": {
    "cntryCd": "AS,CA,GU,JM,MH,TT,UM,VI,BZ,BS,BB",
    "link": "https://weather.com/weather/today/l/{{dynamicLocId}}",
    "default": "https://weather.com/en-GB/weather/today/l/{{dynamicLocId}}"
  },
  "exclude":{
      "cntryCd": "CA"
  }
};
   
    var redirectUrl = '';
    var isNotExcluded = !(otherRules && otherRules.exclude && otherRules.exclude.cntryCd) || otherRules.exclude.cntryCd.indexOf(countryCode)=== -1;
    if ((!destinationUrls || (destinationUrls !== "")) && isNotExcluded) {
      redirectUrl = destinationUrls[destinationSiteCode] || '';
      if (destinationSiteCode !== currentLocale && otherRules.hasOwnProperty(languageCode)) {
        var rule = otherRules[languageCode];
        redirectUrl = (rule.cntryCd.indexOf(countryCode) !== -1) ? rule.link : rule.default;
      }
      redirectUrl = redirectUrl.replace('{{dynamicLocId}}', TWC.PcoUtils.getURLlocid());
    }

    //== Core Logic ==//
    var referrer = document.referrer;
    if (referrer || !redirectUrl) {
      removeController()
      return;
    }

    var jStorage = $.jStorage;

    var site = jStorage.get('sitePref');

    if (!site || site.pref !== CURRENT_SITE_CODE) {
      $.cookie("fromStr", fromString, {
        domain: ".weather.com",
        path: "/"
      });

      if (!site) {
        site = {
          pref: destinationSiteCode,
          firstVisit: true
        };

        jStorage.listenKeyChange('sitePref', function() {
          window.location.href = redirectUrl;
        })
        jStorage.set('sitePref', site);

      } else {
        window.location.href = redirectUrl;
      }
    }
  }
  function removeController(){
     var $module = $('#gm-site-pref-redirect-out-logic').closest('[data-twc-controller]');
     $module.removeAttr('data-twc-controller');
  };
  removeController()
 
})(window.jQuery);

</script>
