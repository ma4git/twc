    <div data-ng-show="hasResults()" data-ng-cloak>
      <div class="menu" data-ng-cloak>
        <div data-ng-switch="resultType()">
          <div class="animate-switch" data-ng-switch-when="Recent">
            <h3 class="heading--search" data-pf-translate="Recent Searches" data-pf-translate-context="glomo_base_search"></h3>
            <ul class="recent">
              <li data-twc-typeahead-item="item" data-ng-repeat="item in results | limitTo:10" class="results">
                  <p class="name" data-ng-bind="getPresName($index)"></p>
              </li>
            </ul>
          </div>
          <div class="animate-switch" data-ng-switch-when="Search">
            <!-- <h3>Results</h3> -->
            <ul class="results">
              <li data-twc-typeahead-item="item" data-ng-repeat="item in results | limitTo:10" class="results">
                  <p class="name" data-ng-bind="getPresName($index)"></p>
              </li>
            </ul>
          </div>
          <div class="animate-switch" data-ng-switch-when="NoResult">
            <p class="result-status" data-pf-translate="No results found." data-pf-translate-context="glomo_base_search"></p>
          </div>
          <div class="animate-switch" data-ng-switch-when="Suppressed">
