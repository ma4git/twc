(function($) {
  var URL_LOCALE_REGREX = /^\/.*?\/$/,
      SPANISH_US_CODE = 'es-US',
      DEFAULT_SITE_CODE = 'en-GB',
      CURRENT_SITE_CODE = 'en-US';

