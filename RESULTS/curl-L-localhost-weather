<script>window.googletag = window.googletag || {};googletag.cmd = googletag.cmd || [];</script>
<script>
      (function () {
        // Define storage
        var storage = window.localStorage || {};

        // Reassign console to placeholder
        var consoleHolder = console;

        TWC.debugEnabled = function () {
          var initDebug = false;

          if (storage.getItem && (!storage.getItem('twcDebug') || storage.getItem('twcDebug') === 'false')) {
            initDebug = false;

            // Override with TWC.Configs.debug option if initDebug is false
            initDebug = Boolean(TWC.Configs.debug);
          } else {
            initDebug = true;
          }

          return initDebug;
        };

        TWC.debug = function (enabled){
        // check for local storage . This is mainly to fix safari throwing a fit in private mode
          if(storage && storage.length > 0){
            storage.setItem && storage.setItem('twcDebug', enabled);
          }
          if (!enabled){
            consoleHolder = console;
            console = {};
            console.log = function () {};
            console.debug = function () {};
            console.info = function () {};
            console.error = function () {};
            console.warn = function () {};
            console.dir = function () {};
            console.table = function () {};
          } else {
            console = consoleHolder;
          }
        }

        // Disable console messaging

        TWC.debug(TWC.debugEnabled());
      })();

      TWC.pco=TWC.initpco(
        jQuery,
        {page:{
          'ampm':       'pm',
          'timezone':   'EDT',
          'serverdate': '08/08/2016',
          'servertime': '23:11:05',
          'locale':     'en-US',
          'serverhrs':  '23',
          'lang':       'en',
          'serverday':  'Mon'
        },ad:{}},
        null,
        null,
        '{"error":"LOCATION_UNAVAILABLE"}'
      );

      (function($){
        $(window).load(function () {
          TWC && TWC.pco && TWC.pco && TWC.pco.setNodeValue('bar','percentage',100);
          window.bar && bar.timer('homepageload.display', 'Time it takes for window load to finish', windowRenderStartTime).end();
          TWC && TWC.Events && TWC.Events.getEvent('barReady').resolve();
        });

         if (window.deferred_ads && window.deferred_ads.targetPages &&
            window.deferred_ads.targetPages.length && window.deferred_ads.targetPages.length > 0) {
            var matchFound = false;
            var targetPagesList = window.deferred_ads.targetPages.split(',');
            for (var i = 0; i < targetPagesList.length; i++) {
                var regexp = new RegExp(targetPagesList[i], 'i');
                if (regexp.test(window.location.href)) {
                    matchFound = true;
                    break;
                }
            }

            // no match found if broke out of loop early
            // set deferred_ads.enabled to false
            if (!matchFound) {
                window.deferred_ads.enabled = false;
            }
        }

        if (window.deferred_ads && window.deferred_ads.enabled) {
            setTimeout(function () {
                if (TWC.Events.getEvent('window_loadReady').state() !== 'resolved') {
                    TWC.Events.getEvent('window_loadReady').resolve();
                    console.debug('window_loadReady resolved via callback');
                }
            }, window.deferred_ads.timeout);

            $(window).load(function () {
                if (TWC.Events.getEvent('window_loadReady').state() !== 'resolved') {
                    TWC.Events.getEvent('window_loadReady').resolve();
                    console.debug('window_loadReady resolved via window load');
                }
            });
        } else {
            TWC.Events.getEvent('window_loadReady').resolve();
        }

        TWC.Events.ifReady(['utag_dataReady']).done(function(){
          // Record third party ad timing for prop68
            utag_data.thirdPartyAdTiming = 'amzn:' + (utag_data['adtiming.amznslotsRequest'] || 'timeout')  +
                                           '|crit:' + (utag_data['adtiming.criteoRequest'] || 'timeout') +
                                           '|lot:' + (utag_data['adtiming.lotameRequest'] || 'timeout')+
                                           '|prepOpex:' + (utag_data['adtiming.openx-OX_dfp_ads_Ready'] || 'timeout') +
                                           '|opex:' + (utag_data['adtiming.openxRequest'] || 'timeout') +
                                           '|wfxtg:' + (utag_data['adtiming.wfxtgRequest'] || 'timeout');
          utag.view(utag_data, null, ['22']);
        });

        TWC.Events.ifReady(TWC.tealium.events).done(function(){
          TWC.PcoUtils.setTiming('tealiumUtagView', (new Date()).getTime());
          if (!window.callPhantom && window.utag && 'en-US' !== 'de-DE') {
            utag.view(utag_data, null, ['140']);
          }
        });

      })(jQuery);</script>
