      languageCode = languageCode.substring(0,2);

  var currentUrlLocale = (window.location.pathname).substring(0, 7),
      isUsUrl = currentUrlLocale === '/' || !URL_LOCALE_REGREX.test(currentUrlLocale) || currentUrlLocale.charAt(3) !== '-';

  var isSpanishUsLocale = (languageCode + '-' + countryCode) === SPANISH_US_CODE,
      isCountryCodeValid = countryCode && countryCode.length === 2 && (!countryCode.match(/US/) || isSpanishUsLocale);

  if (isCountryCodeValid && isUsUrl && !window.location.search.match(/dest=US/)) {
    var currentLocale = languageCode + '-' + countryCode;
    if (languageCode && languageCode.indexOf('-') !== -1) {
        languageCode = languageCode.split('-')[0];
    }

    //== Get destination site code ==//
    var destinationSiteCode = DEFAULT_SITE_CODE;
    $.each(locales, function(key, locale) {

      if (!locale.language || (locale.language && locale.language.indexOf('-') === -1)) {
        removeController()
        return;
      }

      if (locale.language.split('-')[1].indexOf(countryCode) !== -1) {
        var matchedLocale = locales[languageCode + '-' + countryCode];
        destinationSiteCode = matchedLocale ? matchedLocale.language : null;
        removeController()
        return false;
      }
    });

    //== Settings ==//
    var fromString = "weather_usa";
    var destinationUrls = {
"de-DE": "https://weather.com/de-DE",
"en-GB": "https://weather.com/en-GB",
"pt-BR" : "https://weather.com/pt-BR",
	"ja-JP" : "https://weather.com/ja-JP",
	"ko-KR" : "https://weather.com/ko-KR",
	"pl-PL" : "https://weather.com/pl-PL",
	"it-IT" : "https://weather.com/it-IT",
	"ms-MY" : "https://weather.com/ms-MY",
	"hu-HU" : "https://weather.com/hu-HU",
	"el-GR" : "https://weather.com/el-GR",
	"hi-IN" : "https://weather.com/hi-IN",
	"id-ID" : "https://weather.com/id-ID",
	"ru-RU" : "https://weather.com/ru-RU",
	"zh-TW" : "https://weather.com/zh-TW",
	"zh-CN" : "https://weather.com/zh-CN",
	"th-TH" : "https://weather.com/th-TH",
	"tr-TR" : "https://weather.com/tr-TR",
	"uk-UA" : "https://weather.com/uk-UA",
	"vi-VN" : "https://weather.com/vi-VN",
	"sv-SE" : "https://weather.com/sv-SE",
	"es-US" : "https://weather.com/es-US",
	"ro-RO" : "https://weather.com/ro-RO",
	"fr-FR" : "https://weather.com/fr-FR",
	"sk-SK" : "https://weather.com/sk-SK",
	"es-ES" : "https://weather.com/es-ES",
	"pt-PT" : "https://weather.com/pt-PT",
	"no-NO" : "https://weather.com/no-NO",
	"fi-FI" : "https://weather.com/fi-FI",
	"nl-NL" : "https://weather.com/nl-NL",
	"da-DK" : "https://weather.com/da-DK",
	"en-IN" : "https://weather.com/en-IN",
	"cs-CZ" : "https://weather.com/cs-CZ",
	"hr-HR" : "https://weather.com/hr-HR",
	"ca-ES" : "https://weather.com/ca-ES"
};
    var otherRules = {
  "es": {
    "cntryCd": "AR,BO,CL,CO,CR,DO,EC,SV,GQ,GT,HN,MX,NI,PA,PY,PE,PR,US,UY,VE",
    "link": "https://weather.com/es-US ",
    "default": "https://weather.com/es-ES"
  },
  "en": {
    "cntryCd": "AS,CA,GU,JM,MH,TT,UM,VI,US,BZ,BS,BB,PR",
    "link": "https://weather.com",
    "default": "https://weather.com/en-GB"
  },
  "exclude":{
      "cntryCd": "CA"
  }
};
   
    var redirectUrl = '';
    var isNotExcluded = !(otherRules && otherRules.exclude && otherRules.exclude.cntryCd) || otherRules.exclude.cntryCd.indexOf(countryCode)=== -1;
    if ((!destinationUrls || (destinationUrls !== "")) && isNotExcluded) {
      redirectUrl = destinationUrls[destinationSiteCode] || '';
      if (destinationSiteCode !== currentLocale && otherRules.hasOwnProperty(languageCode)) {
        var rule = otherRules[languageCode];
        redirectUrl = (rule.cntryCd.indexOf(countryCode) !== -1) ? rule.link : rule.default;
      }
      redirectUrl = redirectUrl.replace('{{dynamicLocId}}', TWC.PcoUtils.getURLlocid());
    }

    //== Core Logic ==//
    var referrer = document.referrer;
    if (referrer || !redirectUrl) {
      removeController()
      return;
    }

    var jStorage = $.jStorage;

    var site = jStorage.get('sitePref');

    if (!site || site.pref !== CURRENT_SITE_CODE) {
      $.cookie("fromStr", fromString, {
        domain: ".weather.com",
        path: "/"
      });

      if (!site) {
        site = {
          pref: destinationSiteCode,
          firstVisit: true
        };

        jStorage.listenKeyChange('sitePref', function() {
          window.location.href = redirectUrl;
        })
        jStorage.set('sitePref', site);

      } else {
        window.location.href = redirectUrl;
      }
    }
  }
  function removeController(){
     var $module = $('#gm-site-pref-redirect-out-logic').closest('[data-twc-controller]');
     $module.removeAttr('data-twc-controller');
  };
  removeController()
 
})(window.jQuery);

